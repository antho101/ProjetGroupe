<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projetgroupe"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-feature
        android:name="android.hardware.sensor.accelerometer"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivityPrincipale"
            android:label="@string/title_activity_accueil" >
        </activity>
        <activity
            android:name=".ActivityInscription"
            android:label="@string/title_activity_inscription" >
        </activity>
        <activity
            android:name=".NoteDetail"
            android:label="@string/title_activity_note_detail" >
        </activity>
        <activity
            android:name=".ActivityNote"
            android:label="@string/title_activity_activity_note" >
        </activity>
        <activity
            android:name=".EcrireNote"
            android:label="@string/title_activity_ecrire_note" >
        </activity>
        <activity
            android:name=".ActivityNoteListe"
            android:label="@string/title_activity_activity_note_liste" >
        </activity>
        <activity
            android:name=".LinedEditText"
            android:label="@string/title_activity_lined_edit_text" >
        </activity>
    </application>

</manifest>
